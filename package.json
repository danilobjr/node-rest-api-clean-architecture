{
  "name": "eca",
  "version": "0.1.0",
  "description": "Node.js Rest API (Express.js, MongoDB, and Clean Architecture)",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "dev": "tsnd -r tsconfig-paths/register --ignore-watch node_modules --respawn src/index.ts",
    "start": "pm2-runtime .build/src/index.js",
    "openapi": "npx rimraf ./docs/api.json && node ./docs/convertYamlToJson.js",
    "clear-build-folder": "npx rimraf .build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danilobjr/node-rest-api-clean-architecture.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "mongoose"
  ],
  "author": "Danilo Barros <danilobjr@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "git+https://github.com/danilobjr/node-rest-api-clean-architecture/issues"
  },
  "homepage": "git+https://github.com/danilobjr/node-rest-api-clean-architecture#readme",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "cep-promise": "4.1.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "form-data": "4.0.0",
    "helmet": "4.6.0",
    "isomorphic-dompurify": "0.15.0",
    "js-yaml": "3.14.0",
    "jsonwebtoken": "8.5.1",
    "luxon": "1.28.0",
    "mongoose": "5.13.2",
    "morgan": "1.10.0",
    "ms": "2.1.3",
    "node-fetch": "2.6.1",
    "ramda": "0.27.1",
    "random-number-csprng": "1.0.2",
    "string-mask-jedi": "3.1.1",
    "swagger-ui-express": "4.1.6",
    "translitro": "0.2.3",
    "validator": "13.6.0"
  },
  "devDependencies": {
    "@adius/yaml2json": "1.0.1",
    "@types/bcryptjs": "2.4.2",
    "@types/compression": "1.7.1",
